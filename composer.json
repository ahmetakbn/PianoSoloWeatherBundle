{
    "name": "codebach/weather-bundle",
    "description": "Weather Bundle of Symfony",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ahmet Akbana",
            "email": "ahmetakbana@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": { "Codebach\\WeatherBundle\\": "" },
